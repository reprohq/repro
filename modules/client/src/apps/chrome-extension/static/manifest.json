{
  "manifest_version": 3,

  "name": "Repro",
  "description": "Reproduce and fix bugs, fast.",
  "version": "1.0",

  "action": {
    "default_title": "Repro",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "scripting"
  ],

  "web_accessible_resources": [
    {
      "resources": ["rc.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
